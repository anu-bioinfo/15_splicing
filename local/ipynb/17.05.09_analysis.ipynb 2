    "    print '\\nBRCA2 low, SMN1 hi'\n",
    "    print df3.head()\n",
    "    df3.to_csv('brca2_low_smn1_hi.csv')   \n",
    "\n",
    "if any(df4):\n",
    "    print '\\nBRCA2 low, SMN1 mid'\n",
    "    print df4.head()\n",
    "    df4.to_csv('brca2_low_smn1_mid.csv')\n",
    "    plt.plot(df4[xcol],df4[ycol],'.',label='brca2_low_smn1_mid',markersize=markersize)\n",
    "\n",
